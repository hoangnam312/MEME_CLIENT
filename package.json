    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^12.2.2",
